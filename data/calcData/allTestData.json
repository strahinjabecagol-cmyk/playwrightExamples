{
    "basicOperations": {
        "addition": {
            "num1": "5",
            "num2": "5",
            "expected": "10"
        },
        "substraction": {
            "num1": "8",
            "num2": "4",
            "expected": "4"
        },
        "multiply": {
            "num1": "8",
            "num2": "4",
            "expected": "32"
        },
        "division": {
            "num1": "10",
            "num2": "2",
            "expected": "5"
        },
        "concatination": {
            "num1": "10",
            "num2": "2",
            "expected": "102"
        }
    },
    "negativeTests": {
        "nonNumericFirst": {
            "num1": "abc",
            "num2": "5",
            "expectedError": "Number 1 is not a number"
        },
        "nonNumericSecond": {
            "num1": "10",
            "num2": "xyz",
            "expectedError": "Number 2 is not a number"
        },
        "divisionByZero": {
            "num1": "10",
            "num2": "0",
            "expectedError": "Divide by zero error"
        }
    },
    "edgeCases": {
        "zeroValues": {
            "addZeros": {
                "num1": "0",
                "num2": "0",
                "expected": "0"
            },
            "multiplyByZero": {
                "num1": "5",
                "num2": "0",
                "expected": "0"
            },
            "divideZero": {
                "num1": "0",
                "num2": "5",
                "expected": "0"
            }
        },
        "negativeNumbers": {
            "addNegative": {
                "num1": "-5",
                "num2": "3",
                "expected": "-2"
            },
            "multiplyNegatives": {
                "num1": "-4",
                "num2": "-3",
                "expected": "12"
            },
            "divideNegative": {
                "num1": "-10",
                "num2": "2",
                "expected": "-5"
            },
            "concatenateNegatives": {
                "num1": "-10",
                "num2": "-20",
                "expected": "-10-20"
            }
        },
        "decimalNumbers": {
            "addDecimals": {
                "num1": "5.5",
                "num2": "2.3",
                "expected": "7.8"
            },
            "divideWithDecimalResult": {
                "num1": "10",
                "num2": "3",
                "expectedPattern": "3\\.333"
            },
            "concatenateDecimals": {
                "num1": "5.5",
                "num2": "3.2",
                "expected": "5.53.2"
            }
        },
        "largeNumbers": {
            "multiplyLarge": {
                "num1": "999999",
                "num2": "999999",
                "expected": "999998000001"
            }
        }
    },
    "integersOnlyTests": {
        "divideWithIntegersChecked": {
            "num1": "10",
            "num2": "3",
            "expected": "3"
        },
        "divideWithIntegersUnchecked": {
            "num1": "10",
            "num2": "3",
            "expectedPattern": "3\\.333"
        }
    },
    "clearButtonTest": {
        "num1": "10",
        "num2": "5",
        "expected": "15"
    },
    "operationSwitchTest": {
        "num1": "10",
        "num2": "5",
        "addExpected": "15",
        "multiplyExpected": "50"
    },
    "emptyFieldTests": {
        "emptyFirst": {
            "num1": "",
            "num2": "5",
            "expected": "5"
        },
        "emptySecond": {
            "num1": "5",
            "num2": "",
            "expected": "5"
        }
    }
}
